{
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.executorMap": {
    "cpp": "run_c $fullFileName"
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeLensFontFamily": "Iosevka Curly",
  "editor.cursorBlinking": "solid",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Iosevka Curly",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "svelte": "html"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.tabSize": 4
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "errorLens.delay": 0,
  "errorLens.fontFamily": "Iosevka Curly",
  "errorLens.fontSize": "14",
  "errorLens.enabledDiagnosticLevels": ["error"],
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/node_modules": true,
    "**/target": true,
    "**/*.lock": true,
    "**/*-lock.*": true,
    "**/*.out*": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "projectManager.git.baseFolders": ["~/Code", "~/.dotfiles2"],
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "svelte.enable-ts-plugin": true,
  "terminal.integrated.confirmOnKill": "never",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Iosevka Term Curly",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.profiles.osx": {
    "fish": {
      "path": "/opt/homebrew/bin/fish"
    }
  },
  "vim.easymotion": true,
  "vim.highlightedyank.color": "rgb(255, 255, 255, 0.2)",
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.replaceWithRegister": true,
  "vim.useCtrlKeys": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.nextEditor"],
      "silent": true
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.previousEditor"],
      "silent": true
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.view.explorer"],
      "silent": true
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"],
      "silent": true
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["code-runner.run"],
      "silent": true
    },
    {
      "before": ["g", "R"],
      "commands": ["editor.action.rename"],
      "silent": true
    },
    {
      "before": ["<C-n>"],
      "commands": ["editor.action.marker.nextInFiles"],
      "silent": true
    },
    {
      "before": ["<C-p>"],
      "commands": ["editor.action.marker.prevInFiles"],
      "silent": true
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    }
  ],
  "vim.leader": "<space>",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Aldrico's Gruvbox",
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none"
}
